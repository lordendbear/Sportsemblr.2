{
  "name": "sportsemlbr.v2-server",
  "version": "0.0.1",
  "description": "Server side project",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w ./ --exec \"clear && cross-env NODE_ENV=dev babel-node index.js --presets es2015,stage-0\"",
    "build": "babel ./ -s -D --ignore node_modules -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "npm run lint && npm run unit-test && npm run integration-test",
    "lint": "eslint ./",
    "unit-test": "mocha  --compilers js:babel-core/register ./test/unit/ --recursive --reporter spec ",
    "integration-test": "cross-env NODE_ENV=test mocha --exit --timeout 15000 --compilers js:babel-core/register ./test/integration/ --recursive --reporter spec "
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "",
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "dotenv": "^5.0.1",
    "express": "^4.13.3",
    "express-ws": "^3.0.0",
    "jwt-simple": "^0.5.1",
    "mongoose": "^5.0.9",
    "morgan": "^1.8.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.4",
    "eslint": "^3.1.1",
    "mocha": "^5.0.4",
    "nodemon": "^1.9.2",
    "sinon": "^4.4.6",
    "supertest": "^3.0.0"
  }
}
